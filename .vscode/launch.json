  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Program",
      "program": "${workspaceFolder}/packages/magical-types/experiment/dist/magical-types.cjs.js",
      "outFiles": ["${workspaceFolder}/**/*.js"]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Next",
      "program": "${workspaceFolder}/test-app/node_modules/.bin/next",
      "outFiles": ["${workspaceFolder}/**/*.js"],
      "args": ["dev", "test-app"]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Babel",
      "program": "${workspaceFolder}/node_modules/.bin/babel",
      "outFiles": ["${workspaceFolder}/**/*.js"],
      "args": ["./comp.tsx"]
    }
  ]
}
